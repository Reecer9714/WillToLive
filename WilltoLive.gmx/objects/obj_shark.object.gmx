            <string>///Skark Ai

//Island avoid
if(!islandAvoid(256)){

//Find Food
if hunger &gt; 15 {
    food = instance_nearest(x,y,obj_fish)
    if(hunger &gt; 45 &amp;&amp; distance_to_object(obj_player) &lt; distance_to_object(food) &amp;&amp;
        !collision_line(x,y,obj_player.x,obj_player.y,obj_sand,false,true)){
        food = obj_player;
        if distance_to_object(food)&lt;=range{
            move_towards_object(food, 4);
            if place_meeting(x,y,food){
                hunger -= 45;
                obj_player.hp -= 25;
            }
        }
    }else{
        if(!collision_line(x,y,food.x,food.y,obj_sand,false,true)){
            if(!food.colliding &amp;&amp; distance_to_object(food) &lt;= range){
                if(ableToHunt){
                    move_towards_object(food, 4);
                    if(alarm[4] == -1) alarm[4] = room_speed * 4;
                }
                if(place_meeting(x,y,food)){
                    hunger -= 15;
                    instance_destroy(food);
                }
            }
        }
    }
}

}

